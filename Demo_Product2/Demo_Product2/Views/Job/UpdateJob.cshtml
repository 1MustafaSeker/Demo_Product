@model EntityLayer.Concrete.Job
@{
    ViewData["Title"] = "UpdateJob";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}
<form method="post">
    @Html.Label("Meslek Id")
    @Html.TextBoxFor(j=>j.Id,new {@class="form-control"})
    <br />
    @Html.Label("Meslek Ad")
    @Html.TextBoxFor(j=>j.Name,new {@class="form-control"})
    @Html.ValidationMessageFor(j=>j.Name)
    <br />
    <button class="btn btn-primary">Güncelle</button>
</form>

